{
  "name": "frontend",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run copy-static && parcel build 'public/*.html'",
    "dev": "npm run copy-static && parcel serve --port=5000 'public/*.html'",
    "start": "npm run build && npm run dev",
    "copy-static": "mkdir -p dist && cp ./node_modules/@hpcc-js/wasm/dist/graphvizlib.wasm dist/",
    "clean": "rm -rf dist .cache",
    "format": "prettier --write './**/*.{js,css,md,json,html}' --config ./.prettierrc.json --ignore-path ./.prettierignore"
  },
  "dependencies": {
    "@hpcc-js/wasm": "^1.18.0",
    "bootstrap": "^5.2.2",
    "d3": "^7.6.1",
    "d3-graphviz": "^4.4.0",
    "feather-icons": "^4.29.0",
    "save-svg-as-png": "^1.4.17"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.5",
    "@types/d3": "^7.4.0",
    "@types/d3-graphviz": "^2.6.7",
    "@types/feather-icons": "^4.7.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.7.1",
    "prettier-plugin-jsdoc": "^0.4.2"
  }
}
